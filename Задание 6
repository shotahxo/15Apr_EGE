from turtle import *
screensize(1000,1000)
pensize(3)
tracer(100)

right(45)



n=17

down()
right(45)
for i in range(2):
    forward(28*n)
    right(90)
    forward(12*n)
    right(90)
forward(7*n)
left(90)
forward(13*n)
right(90)
for i in range(2):
    forward(10*n)
    right(90)
    forward(30*n)
    right(90)
up()

for x in range(-100,100):
    for y in range(-100,100):
        goto(x*n,y*n)
        dot(3,'red')
done()
